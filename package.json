{
  "name": "integration-core",
  "author": "Warren Hennemuth",
  "keywords": [
    "integration",
    "typescript",
    "javascript"
  ],
  "license": "ISC",
  "description": "A nodejs typescript library of abstract components and interfaces that provide baseline functionality for determining and pushing changes between 2 sets of data to a target system.",
  "repository": {
    "type": "git",
    "url": "https://github.com/whennemuth/integration-core.git"
  },
  "version": "1.0.0",
  "module": "dist/esm/index.js",
  "main": "dist/cjs/index.js",
  "types": "dist/types/bin/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist/ && node bin/build.js",
    "build:tsc": "rm -rf dist/ && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outdir dist/cjs",
    "pack": "npm run build && npm pack --pack-destination .",
    "publish": "npm run pack && npm publish ./integration-core-1.0.0.tgz",
    "watch": "tsc -w",
    "test": "jest --silent",
    "test-harness": "ts-node ./test/test-harness/Runner.ts",
    "sqllite": "ts-node ./test/test-harness/SqlLiteDb.ts"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.7.2",
    "@types/pg": "^8.15.6",
    "esbuild": "^0.25.10",
    "jest": "^30.2.0",
    "pg-mem": "^3.0.5",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.939.0",
    "@aws-sdk/lib-storage": "^3.939.0",
    "better-sqlite3": "^12.4.1",
    "humanize-duration": "^3.33.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.27"
  },
  "overrides": {
    "tar": "^7.5.6"
  }
}
